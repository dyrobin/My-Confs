Host me.github.com
    User git
    HostName github.com
    IdentityFile ~/.ssh/github-me.key

Host dcs.github.com
    User git
    HostName github.com
    IdentityFile ~/.ssh/github-dcs.key

Host kanssani
    User kanssani
    HostName cloud21.hostingpalvelu.fi
    Port 5757
    IdentityFile ~/.ssh/kanssani.key

Host otax
    User cssa
    HostName otax.ayy.fi
    IdentityFile ~/.ssh/otax.key


