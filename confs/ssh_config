Host git.cs.hut.fi
    User git
    IdentityFile ~/.ssh/gitcs.key

Host git.niksula.hut.fi
    User git
    IdentityFile ~/.ssh/gitniksula.key

Host mannerheim
    HostName mannerheim.cs.hut.fi
    User yangdeng
    IdentityFile ~/.ssh/mannerheim.key

# tunnel to intranet of CSE (needs to set socks proxy manually)
Host tunnel
    HostName mannerheim.cs.hut.fi
    User yangdeng
    DynamicForward 8080
    IdentityFile ~/.ssh/mannerheim.key


