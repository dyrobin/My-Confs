# git repository
Host bitbucket.helvar.tools
    User git
    IdentityFile ~/.ssh/helvar.key

Host github.com
    User git
    IdentityFile ~/.ssh/github.key

Host gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab.key

# account login
Host kanssani
    User kanssani
    HostName cloud21.hostingpalvelu.fi
    Port 5757
    IdentityFile ~/.ssh/kanssani.key


